{{#.}}
<!--<li class="list-group-title">{{letter}}</li>-->
	{{#each list}}
		<li data-id="{{id}}" class="group-item">
			<a href="message.html?id={{id}}" class="item-link">
				<div class="item-content">
					<div class="item-media"><img src="img/cats/{{picId}}.png" width="44"></div>
					<div class="item-inner">
						<div class="item-title-row">
							<div class="item-title">{{groupName}}</div>
							<div class="item-subtitle">{{id}}</div>
						</div>
						<div class="item-subtitle">Wow, awesome!</div>
					</div>
				</div>
			</a>
		</li>
	{{/each}}
{{/.}}